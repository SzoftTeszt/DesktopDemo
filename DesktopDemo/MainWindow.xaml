<Window x:Class="DesktopDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Canvas>
        <Button Canvas.Left="10" Canvas.Top="10" Click="Button_Click">Button 1</Button>
        <Button Canvas.Right="10" Canvas.Top="10" Click="Button_Click_1">Button 2</Button>
        <Button Canvas.Left="10" Canvas.Bottom="10">Button 3</Button>
        <Button Canvas.Right="10" Canvas.Bottom="10" >Button 4</Button>
        <Ellipse Fill="Azure" Canvas.Left="50" Canvas.Top="50" Width="200" Height="200"></Ellipse>
        <Rectangle Fill="LightBlue" Canvas.Left="50" Canvas.Top="50"  Width="75" Height="75"></Rectangle>   
        <Rectangle Fill="LightCoral" Panel.ZIndex="1" Canvas.Left="75" Canvas.Top="75"  Width="75" Height="75"></Rectangle>
        <Rectangle Fill="LightGreen" Canvas.Left="100" Canvas.Top="100"  Width="75" Height="75"></Rectangle>
    </Canvas>
</Window>
